{
  "name": "happyyogi",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run",
    "test:setup": "vitest run --config setup/vitest.config.ts",
    "db:migrate": "tsx src/lib/server/db/migrate.ts",
    "db:generate": "kysely-codegen --dialect postgres --out-file src/lib/server/db/generated.ts",
    "db:seed": "tsx src/lib/server/db/seed.ts"
  },
